{
    "$schema": "reportThemeSchema-2.114.json",
    "name": "Nord Theme for Power BI",
    "dataColors": ["#6C81A8", "#8CA0BE", "#9BBFCE", "#9DBBBA", "#BF8A74", "#AB90AB", "#505668", "#DADEE8", "#3599B8", "#DFBFBF", "#4AC5BB", "#5F6B6D", "#FB8281", "#F4D25A", "#7F898A", "#A4DDEE", "#FDAB89", "#B687AC", "#28738A", "#A78F8F", "#168980", "#293537", "#BB4A4A", "#B59525", "#475052", "#6A9FB0", "#BD7150", "#7B4F71", "#1B4D5C", "#706060", "#0F5C55", "#1C2325"],
    "bad": "#AB666C",
    "neutral": "#E1CC93",
    "good": "#AABD91",
    "minimum": "#E6E9EF",
    "center": "#505668",
    "maximum": "#3E4251",
    "foreground": "#30343F",
    "foregroundNeutralSecondary": "#505668",
    "backgroundLight": "#E6E9EF",
    "foregroundNeutralTertiary": "#DADEE8",
    "backgroundNeutral": "#EDEFF3",
    "accent": "#8CA0BE",
    "textClasses": {
        "callout": {
            "fontSize": 32,
            "fontFace": "Segoe UI Bold",
            "color": "#30343F"
        },
        "header": {
            "fontSize": 12,
            "fontFace": "Segoe UI",
            "color": "#464C5C"
        },
        "title": {
            "fontSize": 12,
            "fontFace": "Segoe UI Semibold",
            "color": "#505668"
        },
        "largeTitle": {
            "fontSize": 14,
            "fontFace": "Segoe UI Semibold",
            "color": "#505668"
        },
        "label": {
            "fontSize": 10,
            "fontFace": "Segoe UI",
            "color": "#30343F"        
        },
        "semiboldLabel": {
            "fontSize": 12,
            "fontFace": "Segoe UI Semibold",
            "color": "#30343F"
        },
        "largeLabel": {
            "fontSize": 14,
            "fontFace": "Segoe UI",
            "color": "#30343F"           
        },
        "smallLabel": {
            "fontSize": 9,
            "fontFace": "Segoe UI",
            "color": "#30343F"
        },
        "lightLabel": {
            "fontSize": 9,
            "fontFace": "Segoe UI",
            "color": "#30343F"        
        },
        "boldLabel": {
            "fontSize": 12,
            "fontFace": "Segoe UI Bold",
            "color": "#30343F"            
        },
        "largeLightLabel": {
            "fontFace": "Segoe UI",
            "color": "#30343F",            
            "fontSize": 14
        },
        "smallLightLabel": {
            "fontFace": "Segoe UI",
            "color": "#30343F",            
            "fontSize": 9
        }
    },
    "visualStyles": {
        "*": {
            "*": {
                "legend": [{
					"show": true,
					"position": "Top",
					"showTitle": true,
					"legendMarkerRendering": "markerOnly",
					"matchLineColor": false,
					"defaultToCircle": false
				}],
                "title": [{
                    "show": true,
                    "background": { "solid": { "color": "#FFFFFF" }},
                    "alignment": "left"
                }],
                "background": [{
                    "show": true,
                    "color": { "solid": { "color": "#FFFFFF" }},
                    "transparency": 0
                }],
                "lockAspect": [{
                    "show": true
                }],
                "border": [{
                    "show": true,
                    "color": { "solid": { "color": "#FFFFFF" }},                    
                    "radius": 8
                }],
                "dropShadow": [{
                    "show": true,
                    "angle": 90,
                    "color" : { "solid": { "color": "#30343F" }},
                    "position": "Outer",
                    "preset": "Custom",
                    "shadowBlur": 16,
                    "shadowDistance": 4,
                    "shadowSpread": 4,
                    "transparency": 84
                }],
                "visualHeader": [{
                    "show": false,
                    "foreground": { "solid": { "color": "#505668" }},
                    "border": { "solid": { "color": "#505668" }},
					"background": { "solid": { "color": "#FFFFFF"}},
					"transparency": "20D",
					"showVisualInformationButton": false,
					"showVisualWarningButton": false,
					"showVisualErrorButton": false,
					"showDrillRoleSelector": false,
					"showDrillUpButton": false,
					"showDrillToggleButton": false,
					"showDrillDownLevelButton": false,
					"showDrillDownExpandButton": false,
					"showPinButton": false,
					"showFocusModeButton": false,
					"showFilterRestatementButton": false,
					"showSeeDataLayoutToggleButton": false,
					"showOptionsMenu": false,
					"showTooltipButton": false
                }],
                "visualTooltip": [{
                    "themedTitleFontColor": { "solid": { "color": "#30343F" }},
                    "themedValueFontColor": { "solid": { "color": "#464C5C" }},
                    "actionFontColor": { "solid": { "color": "#505668" }}
                }],
                "visualHeaderTooltip": [{
                        "transparency": 0
                }]
            }
        },
        "page": {
            "*": {
                "background": [{ 
                    "color": { "solid": { "color": "#EDEFF3" } },
                    "transparency": 0
                }]
            }
        },
        "pivotTable": {
            "*": {
                "stylePreset": [{
                    "name": "Minimal"
                }],
                "grid": [{
					"gridVertical": false,
					"gridVerticalColor": { "solid": { "color": "#E6E7E8"}},
					"gridVerticalWeight": 1,
					"gridHorizontal": true,
					"gridHorizontalColor": { "solid": { "color": "#E6E7E8"}},
					"gridHorizontalWeight": 1,
					"rowPadding": 3,
					"outlineColor": { "solid": { "color": "#8CA0BE"}},
					"outlineWeight": 1,
					"textSize": 12,
					"imageHeight": 100
				}]
            }
        },
        "tableEx": {
            "*": {
                "stylePreset": [{
                    "name": "Minimal"
                }],
                "grid": [{
					"gridVertical": false,
					"gridVerticalColor": { "solid": { "color": "#E6E7E8"}},
					"gridVerticalWeight": 1,
					"gridHorizontal": true,
					"gridHorizontalColor": { "solid": { "color": "#E6E7E8"}},
					"gridHorizontalWeight": 1,
					"rowPadding": 3,
					"outlineColor": { "solid": { "color": "#8CA0BE"}},
					"outlineWeight": 1,
					"textSize": 12,
					"imageHeight": 100
				}]
            }
        }
    }
}